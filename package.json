    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "nodemon": "^2.0.4",
    "source-map-loader": "^0.2.4",
    "stats-webpack-plugin": "^0.7.0",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "express": "^4.17.1",
